{
  "name": "@animadate/admin",
  "license": "MIT",
  "scripts": {
    "dev:app": "npm run -s is-initialized && npm run -s migrate:dev && cross-env FORCE_COLOR=1 next dev --turbo --port ${PORT:-3003}",
    "dev:wss": "tsx --watch src/server/wsServer.ts",
    "build": "npm run -s build:app",
    "build:app": "next build",
    "start:app": "npm run deploy-db:prod && next start --port ${PORT:-3000}",
    "start:wss": "cross-env NODE_ENV=production tsx src/server/wsServer.ts",
    "migrate:dev": "prisma migrate dev --schema=../../packages/app/db/prisma/schema.prisma && prisma migrate dev --schema=../../packages/events/db/prisma/schema.prisma",
    "deploy-db:prod": "prisma migrate deploy --schema=../../packages/app/db/prisma/schema.prisma && prisma migrate deploy --schema=../../packages/events/db/prisma/schema.prisma",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "is-initialized": "cd ../../packages/scripts && npm run is-initialized",
    "analyze": "ANALYZE=true npm run build && cd .next/analyze && npx -y serve",
    "seed": "cross-env NODE_ENV=development prisma db seed"
  },
